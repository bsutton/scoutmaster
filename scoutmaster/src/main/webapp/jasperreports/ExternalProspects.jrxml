	<queryString>
		<![CDATA[select firstname,lastname, phoneno
, concat(a.street, ", ", a.city) as address, n.subject as subject, n.body as body
from Contact c
join Phone p
 on phone1_id = p.id
join Address a
 on c.address_id = a.id
join Contact_Tag ct
	on c.id = ct.contacts_id
join Tag t
	on t.id = ct.tags_id
join Note n
	on n.attachedcontact_id = c.id
where t.name = 'External Prospect'
order by lastname]]>
	</queryString>
