		<dropColumn tableName="tblReportEmailSchedule_tblReportEmailRecipient"
			columnName="UPDATED" />

		<dropColumn
			tableName="tblReportEmailSchedule_tblReportEmailScheduledDateParameter"
			columnName="CONSISTENCYVERSION" />
