<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<!-- Add color to Event. -->

	<changeSet author="bsutton" id="1.0.38">
		<addColumn tableName="Event">
			<column defaultValueNumeric="255" name="red" type="INTEGER(8)" />
			<column defaultValueNumeric="255" name="green" type="INTEGER(8)" />
			<column defaultValueNumeric="255" name="blue" type="INTEGER(8)" />
			<column defaultValueNumeric="255" name="alpha" type="INTEGER(8)" />
		</addColumn>


	</changeSet>

</databaseChangeLog>
